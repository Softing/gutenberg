    <title>Inprint Content 5.0</title>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"></meta>
    <!-- meta HTTP-EQUIV="Pragma" CONTENT="no-cache"></meta -->

    <link href="/favicon.ico" rel="icon" type="image/x-icon" />

    <link type="text/css" rel="stylesheet" href="/ext-3.3.2/resources/css/ext-all.css">
    <link type="text/css" rel="stylesheet" href="/ext-3.3.2/resources/css/xtheme-gray.css">

    <% foreach (@$css) { %>
    <link type="text/css" rel="stylesheet" href="<%= $_ %>"/><% } %>

    <!-- Ext 3.3.0 -->
    <script type="text/javascript" src="/ext-3.3.2/adapter/ext/ext-base-debug.js"></script>
    <script type="text/javascript" src="/ext-3.3.2/ext-all-debug.js"></script>

    <!-- Locale support -->
    <script type="text/javascript" src="/ext-3.3.2/src/locale/ext-lang-ru.js"></script>

</head>
<body>
    <script type="text/javascript" src="/scripts/wallpaper.js"></script>
    <!-- Loading mask element -->
    <div id="loading-mask" style=""></div>
    <div id="loading">
        <img  id="loading-indicator" src="/images/ajax-loader.gif"/>
        Inprint Content 5.0
    </div>

    <script>
        //alert (document.getElementById("loading-mask"));
    </script>

    <!-- Fields required for history management -->
    <form id="history-form" class="x-hidden">
        <input type="hidden" id="x-history-field" />
        <iframe id="x-history-frame"></iframe>
    </form>

    <!-- div class="footer">inprint.content 5.0.0</div -->
</body>
</html>

<!-- Plugins -->
<% foreach (@$cssplugins) { %>
<link type="text/css" rel="stylesheet" href="<%= $_ %>"/><% } %>
<% foreach (@$jsplugins) { %>
<script type="text/javascript" src="<%= $_ %>"></script><% } %>

<!-- WIdgets -->
<script type="text/javascript" src="/locale/"></script>
<% foreach (@$js) { %>
<script type="text/javascript" src="<%= $_ %>"></script><% } %>
